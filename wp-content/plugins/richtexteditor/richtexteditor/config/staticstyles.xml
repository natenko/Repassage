	<style name="gallery-item" cssclass="gallery-item" />
	<style name="image-attachment" cssclass="image-attachment" />
	<style name="page-links" cssclass="page-links" />
	<style name="page-title" cssclass="page-title" />
	<style name="site" cssclass="site" />
	<style name="site-content" cssclass="site-content" />
	<style name="site-header" cssclass="site-header" />
